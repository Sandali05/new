# Copy this file to `.env` and populate the values before running the app.

# OpenAI API key used by the instruction agent (optional if using Groq only)
OPENAI_API_KEY=

# Groq API key used for model inference
GROQ_API_KEY=

# Astra DB vector store configuration
ASTRA_DB_API_ENDPOINT=
ASTRA_DB_KEYSPACE=
ASTRA_DB_DATABASE=
ASTRA_DB_COLLECTION=
ASTRA_DB_APPLICATION_TOKEN=

# Default model preferences
MODEL_PREFERENCE=groq
EMBEDDING_MODEL=text-embedding-3-small
